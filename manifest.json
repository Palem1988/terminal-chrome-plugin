{
  "manifest_version": 2,
  "name": "CryptoControl Plugin",
  "description": "This is test plugin",
  "version": "1.0",
  "background": {
  		"scripts": ["background.js"]
  },
	"browser_action": {
		"default_icon": "image.png",
		"default_title": "Allow-Control-Allow-Origin",
		"default_popup": "popup.html"
	},
  "permissions": [
   "storage",
		"webRequest",
		"webRequestBlocking",
		"<all_urls>"
  ],
  "web_accessible_resources": [
		"off.png", "image.png"
	]
}
