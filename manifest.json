{
    "manifest_version": 2,
    "name": "CryptoControl Terminal",
    "description": "Plugin to be used with the CryptoControl Web Terminal",
    "icons": {
        "16": "cryptocontrol.png",
        "48": "cryptocontrol.png",
        "128": "cryptocontrol.png"
    },
    "homepage_url": "https://github.com/cryptocontrol/terminal-chrome-plugin",
    "version": "1.3.0",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "logo.png",
        "default_title": "CryptoControl"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ]
}